{
   "_id": "_design/Agenda",
   "language": "javascript",
   "views": {
       "list": {
           "map": "function(doc) {\n  if(doc.class && doc.class==\"Agenda\")\n  emit(doc.datep, doc);\n}"
       },
       "actionsToDo": {
           "map": "function(doc) {\nif (doc.class == \"Agenda\" && (!doc.status || doc.status == \"0\"))\n  emit(doc.label, doc);\n}"
       }
   }
}