{
   "_id": "extrafields:Agenda",
   "class": "extrafields",
   "ico": "16-Month-Calendar.png",
   "langs": [
       "agenda",
       "commercial"
   ],
   "fields": {
       "societe": {
           "enable": true,
           "class": "Societe",
           "view": "list",
           "params": {
           }
       },
       "contact": {
           "enable": true,
           "class": "Contact",
           "view": "listSociete",
           "params": {
               "key": "$this->societe->id"
           }
       },
       "Status": {
           "type": "select",
           "enable": true,
           "default": "TODO",
           "status": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "TODO": {
                   "enable": true,
                   "label": "StatusActionToDo",
                   "cssClass": "blue-gradient",
                   "dateEnd": [
                       "expired",
                       "ontime"
                   ]
               },
               "ON": {
                   "label": "StatusActionInProcess",
                   "enable": true,
                   "cssClass": "orange-gradient",
                   "dateEnd": [
                       "expired",
                       "ontime"
                   ]
               },
               "DONE": {
                   "enable": true,
                   "label": "StatusActionDone",
                   "cssClass": "green-gradient"
               },
               "NOTAPP": {
                   "label": "StatusNotApplicable",
                   "enable": false,
                   "cssClass": "grey-gradient"
               },
               "ontime": {
                   "enable": false,
                   "label": "StatusActionToDo",
                   "cssClass": "blue-gradient"
               },
               "expired": {
                   "enable": false,
                   "label": "StatusActionTooLate",
                   "cssClass": "red-gradient"
               }
           }
       },
       "type_code": {
           "type": "select",
           "enable": true,
           "default": "AC_RDV",
           "aoColumns": {
               "sClass": "",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "AC_RDV": {
                   "enable": true,
                   "label": "Rendez-vous",
                   "event": true,
                   "priority": 7
               },
               "AC_TEL": {
                   "enable": true,
                   "label": "Phone call",
                   "event": true,
                   "priority": 6
               },
               "AC_FAX": {
                   "enable": true,
                   "label": "Send Fax",
                   "priority": 1
               },
               "AC_EMAIL": {
                   "enable": true,
                   "label": "Send Email",
                   "priority": 2
               },
               "AC_OTH": {
                   "enable": false,
                   "label": "Other",
                   "priority": 0
               },
               "AC_FAC": {
                   "enable": true,
                   "label": "Send customer invoice by email",
                   "priority": 7
               },
               "AC_SUP_INV": {
                   "enable": true,
                   "label": "Send supplier invoice by email",
                   "priority": 5
               },
               "AC_COM": {
                   "enable": true,
                   "label": "Send customer order by email",
                   "priority": 9
               },
               "AC_SUP_ORD": {
                   "enable": true,
                   "label": "Send supplier order by email",
                   "priority": 5
               },
               "AC_PROP": {
                   "enable": true,
                   "label": "Send commercial proposal by email",
                   "priority": 10
               },
               "AC_SHIP": {
                   "enable": true,
                   "label": "Send shipping by email",
                   "priority": 4
               }
           }
       },
       "author": {
           "type": "select",
           "label": "ActionAskedBy",
           "class": "User",
           "view": "list"
       },
       "usertodo": {
           "type": "select",
           "label": "ActionAffectedTo",
           "class": "User",
           "view": "list"
       },
       "userdone": {
           "type": "select",
           "label": "ActionDoneBy",
           "class": "User",
           "view": "list"
       }
   }
}