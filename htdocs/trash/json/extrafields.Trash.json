{
   "_id": "extrafields:Trash",
   "class": "extrafields",
   "langs": [
       "companies",
       "bills",
       "compta"
   ],
   "pluginsList": [
       "RowAction",
       "DeleteNotification"
   ],
   "longList": [
       "_id",
       "name",
       "Action",
       "Checkbox"
   ],
   "shortList": [
       "name"
   ],
   "fields": {
       "_id": {
           "type": "text",
           "enable": true,
           "list": {
               "visible": false,
               "static": true
           }
       },
       "name": {
           "type": "text",
           "label": "Company",
           "length": 60,
           "enable": true,
           "list": {
               "static": true
           },
           "render": {
               "type": "url",
               "ico": "16-Apartment-Building.png"
           },
           "validate": {
               "cssclass": "required",
               "minlength": 3
           }
       },
       "Action": {
           "type": "action",
           "enable": true,
           "list": {
               "width": 35,
               "cssclass": "center",
               "sortable": false,
               "searchable": false,
               "static": true
           },
           "action": {
               "edit": {
                   "label": "Edit",
                   "cssclass": "icon edit"
               },
               "delete": {
                   "label": "Delete",
                   "cssclass": "icon delete delEnqBtn"
               }
           }
       },
       "Checkbox": {
           "type": "checkbox",
           "label": false,
           "enable": true,
           "list": {
               "width": 8,
               "sortable": false,
               "searchable": false,
               "static": true
           },
           "render": {
           }
       }
   }
}