{
   "_id": "extrafields:Societe",
   "class": "extrafields",
   "ico": "16-Apartment-Building.png",
   "langs": [
       "companies"
   ],
   "longList": [
       "code_client",
       "zip",
       "town",
       "address",
       "phone",
       "idprof1",
       "commercial_id"
   ],
   "shortList": [
       "town",
       "phone",
       "commercial_id"
   ],
   "fields": {
       "name": {
           "type": "input",
           "length": 60,
           "enable": true,
           "validate": {
               "required": true,
               "minlength": 3
           }
       },
       "prefix": {
           "type": "text",
           "length": 5,
           "enable": false,
           "edit": true
       },
       "code_client": {
           "type": "text",
           "length": 15,
           "enable": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": false
           }
       },
       "code_fournisseur": {
           "type": "text",
           "length": 15,
           "enable": false
       },
       "barcode": {
           "type": "text",
           "enable": false
       },
       "contact": {
           "enable": true,
           "class": "Contact",
           "view": "list"
       },
       "Status": {
           "type": "select",
           "enable": true,
           "default": "ST_NEVER",
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": true
           },
           "values": {
               "ST_NO": {
                   "enable": true,
                   "type": "closed",
                   "cssClass": "red-gradient"
               },
               "ST_TODO": {
                   "enable": false,
                   "type": "prsopect",
                   "cssClass": "blue-gardient"
               },
               "ST_NEVER": {
                   "enable": true,
                   "type": "never",
                   "cssClass": "grey-gradient"
               },
               "ST_PCOLD": {
                   "enable": true,
                   "type": "prospect",
                   "cssClass": "blue-gradient"
               },
               "ST_PHOT": {
                   "enable": true,
                   "type": "prospect",
                   "cssClass": "red-gradient"
               },
               "ST_CLESS3": {
                   "enable": true,
                   "type": "customer",
                   "cssClass": "orange-gradient"
               },
               "ST_CUST": {
                   "enable": true,
                   "type": "customer",
                   "cssClass": "green-gradient"
               },
               "ST_CPAR": {
                   "enable": false,
                   "type": "customer",
                   "cssClass": "green-gradient"
               }
           }
       },
       "address": {
           "type": "textarea",
           "cols": 40,
           "rows": 3,
           "enable": true,
           "aoColumns": {
               "sClass": "",
               "bSearchable": true,
               "bVisible": false
           }
       },
       "zip": {
           "type": "text",
           "label": "Zip",
           "length": 15,
           "enable": true,
           "javascript": null,
           "aoColumns": {
               "sClass": "center dol_edit",
               "bSearchable": true,
               "bVisible": true
           }
       },
       "town": {
           "type": "text",
           "label": "Town",
           "length": 15,
           "enable": true,
           "javascript": null,
           "aoColumns": {
               "sClass": "dol_edit",
               "bSearchable": true,
               "bVisible": true
           }
       },
       "country_id": {
           "type": "select",
           "enable": true,
           "dict": "dict:country",
           "default": "substr($langs->defaultlang,3,5)"
       },
       "state_id": {
           "type": "select",
           "enable": true,
           "dict": "dict:fk_departements"
       },
       "DefaultLang": {
           "type": "select",
           "enable": false,
           "dict": null
       },
       "logo": {
           "type": "uploadfile",
           "enable": true
       },
       "phone": {
           "type": "AC_TEL",
           "ico": "AC_TEL",
           "enable": true,
           "aoColumns": {
               "sClass": "center",
               "bSearchable": true,
               "bVisible": false
           },
           "mask": "+99 9 99 99 99 99"
       },
       "fax": {
           "type": "AC_FAX",
           "ico": "AC_FAX",
           "enable": true,
           "mask": "+99 9 99 99 99 99"
       },
       "email": {
           "type": "AC_EMAIL",
           "ico": "AC_EMAIL",
           "enable": true,
           "validate": {
               "email": true
           }
       },
       "url": {
           "type": "text",
           "ico": "AC_URL",
           "site": "www",
           "enable": true,
           "validate": {
               "url": true
           }
       },
       "idprof1": {
           "type": "text",
           "label": "ProfId1",
           "length": "25",
           "enable": true,
           "aoColumns": {
               "bSearchable": true,
               "bVisible": true
           }
       },
       "idprof3": {
           "type": "text",
           "length": "12",
           "enable": true
       },
       "typent_id": {
           "type": "select",
           "default": "TE_UNKNOWN",
           "enable": true,
           "values": {
               "TE_UNKNOWN": {
                   "enable": true
               },
               "TE_ADMIN": {
                   "enable": true
               },
               "TE_OTHER": {
                   "enable": true
               },
               "TE_FEMME": {
                   "enable": false
               },
               "TE_GROUP": {
                   "enable": true
               },
               "TE_WHOLE": {
                   "enable": false
               },
               "TE_HOMME": {
                   "enable": false
               },
               "TE_PRIVATE": {
                   "enable": true
               },
               "TE_MEDIUM": {
                   "enable": true
               },
               "TE_RETAIL": {
                   "enable": false
               },
               "TE_STARTUP": {
                   "enable": false
               },
               "TE_SMALL": {
                   "enable": true
               }
           }
       },
       "effectif_id": {
           "type": "select",
           "enable": true,
           "default": "EF0",
           "values": {
               "EF0": {
                   "enable": true,
                   "label": "-"
               },
               "EF1-5": {
                   "enable": true,
                   "label": "1 - 5"
               },
               "EF6-10": {
                   "enable": true,
                   "label": "6 - 10"
               },
               "EF11-50": {
                   "enable": true,
                   "label": "11 - 50"
               },
               "EF51-100": {
                   "enable": true,
                   "label": "51 - 100"
               },
               "EF101-500": {
                   "enable": true,
                   "label": "101 - 500"
               },
               "EF500-": {
                   "enable": true,
                   "label": "> 500"
               }
           }
       },
       "capital": {
           "type": "text",
           "length": "20",
           "label": "Capital",
           "enable": "1",
           "spinner": "min: 0"
       },
       "VATIsUsed": {
           "type": "checkbox",
           "enable": true,
           "default": true
       },
       "VATIntra": {
           "type": "text",
           "length": 25,
           "javascript": null,
           "enable": true
       },
       "forme_juridique_code": {
           "type": "select",
           "label": "JuridicalStatus",
           "dict": "dict:fk_forme_juridique",
           "enable": true
       },
       "commercial_id": {
           "label": "SalesRepresentatives"
       },
       "civilite_id": {
           "type": "select",
           "label": "UserTitle",
           "default": "NO",
           "dict": "dict:fk_civilite",
           "enable": true
       }
   }
}